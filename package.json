{
  "name": "obsidian-plugin-update-tracker",
  "version": "1.2.0",
  "description": "Know when installed plugins have updates and evaluate the risk of upgrading",
  "main": "main.js",
  "scripts": {
    "setup": "npm i && git config core.hooksPath .githooks/",
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "downgrade-self": "sh scripts/change-plugin-version.sh obsidian-plugin-update-tracker 0.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@codemirror/state": "^6.0.0",
    "@codemirror/view": "^6.0.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^29.0.3",
    "@types/node": "^16.11.6",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/redux-logger": "^3.0.9",
    "@types/styled-components": "^5.1.26",
    "babel-loader": "^8.2.5",
    "builtin-modules": "3.3.0",
    "dotenv": "^16.0.2",
    "esbuild": "0.14.47",
    "jest": "^29.0.3",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.8.5",
    "dayjs": "^1.11.5",
    "lodash": "^4.17.21",
    "obsidian": "^0.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "react-redux": "^8.0.2",
    "redux-logger": "^3.0.6",
    "styled-components": "^5.3.5",
    "use-change-aware-effect": "^1.0.1"
  }
}
